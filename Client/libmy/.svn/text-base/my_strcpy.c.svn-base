char	*my_strcpy(char *dest, char *src)
{
  char	*ptr_tmp;

  ptr_tmp = dest;
  while (*src != '\0')
    {
      *dest = *src;
      src++;
      dest++;
    }
  *dest = '\0';
  dest = ptr_tmp;
  return (dest);
}

